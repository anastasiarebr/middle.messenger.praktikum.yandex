{
    "extends": "eslint:recommended",
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
      "max-len": [2, 120],
      "@typescript-eslint/no-unused-vars": "off",
      "no-undef":["warn"],
      "no-unused-vars": ["warn"]
    },
    "env": {
        "node": true,
        "browser": true,
        "es2021": true
    }
}